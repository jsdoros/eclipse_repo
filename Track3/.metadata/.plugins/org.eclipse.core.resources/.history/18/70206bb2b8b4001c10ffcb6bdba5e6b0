package org.ssglobal.training.codes.util;

import java.util.Arrays;

public class TestArrays {

	public static void main(String[] args) {
				
		double[] temp = new double [60];
		Arrays.fill(temp, -1.0);
		for(double lookup: temp) {
			System.out.format("%f ", lookup);
		}
		System.out.println();
		
		double[] grades = new double[] {100.0, 90.1, 80.2, 0.3, 60.4, 50.5};
		double[] gradestemp = new double[0];
		
		Arrays.copyOf(grades, grades.length);
		
	}

}
